<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
        "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<urlrewrite>
    <rule>
        <from>^/page/(\w+)$</from>
        <to type="redirect">/page/$1.html</to>
    </rule>
    <rule>
        <from>^/demo/(\w+)/(\w+).html$</from>
        <to type="redirect" >/page/$1.action?id=$2</to>
    </rule>
    <rule>
        <from>/home</from>
        <to>/index.jsp</to>
    </rule>
</urlrewrite>